      {
        "pagePath": "pages/routine/routine",
        "text": "Routine",
        "iconPath": "assets/icons/routine.png",
        "selectedIconPath": "assets/icons/routine-active.png"
      },
      {
        "pagePath": "pages/explore/explore",
        "text": "Explore",
        "iconPath": "assets/icons/explore.png",
